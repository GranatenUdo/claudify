  "selectionRules": {
    "minimal": {
      "description": "Core essentials only",
      "includes": [
        "agents:code-reviewer",
        "commands:essential:*",
        "commands:documentation:update-changelog"
      ]
    },
    "standard": {
      "description": "Recommended based on tech stack",
      "includes": [
        "minimal:*",
        "agents:security-reviewer",
        "agents:[frontend-developer if frontend]",
        "commands:backend:* if backend",
        "commands:frontend:* if frontend",
        "commands:quality:analyze-technical-debt",
        "generators:command-generator"
      ]
    },
    "comprehensive": {
      "description": "Everything available",
      "includes": ["*"]
    }
  },
  "techStackMappings": {
    "backend": {
      "dotnet": {
        "patterns": ["*.csproj", "*.sln"],
        "recommendedAgents": ["code-reviewer", "security-reviewer", "tech-lead"],
        "recommendedCommands": ["backend:*", "quality:*"]
      },
      "nodejs": {
        "patterns": ["package.json", "node_modules"],
        "recommendedAgents": ["code-reviewer", "security-reviewer"],
        "recommendedCommands": ["backend:*", "quality:*"]
      }
    },
    "frontend": {
      "angular": {
        "patterns": ["angular.json", "@angular/core"],
        "recommendedAgents": ["frontend-developer", "ux-reviewer"],
        "recommendedCommands": ["frontend:*", "quality:*"]
      },
      "react": {
        "patterns": ["react", "*.tsx", "*.jsx"],
        "recommendedAgents": ["frontend-developer", "ux-reviewer"],
        "recommendedCommands": ["frontend:*", "quality:*"]
      }
    }
  }
